package com.capgemini.set2;

public class Q66 {
	public static void main(String[] args) {
		int ans;
		try {
			int num = 10;
			int div = 0;
			ans = num/div;
			System.out.println(ans);
		} catch(ArithmeticException ae) {
			ans = 0;
		} catch (Exception e) {
			System.out.println("Invalid Calculation");
		}
		System.out.println("Answer ="+ans);
	}
}
