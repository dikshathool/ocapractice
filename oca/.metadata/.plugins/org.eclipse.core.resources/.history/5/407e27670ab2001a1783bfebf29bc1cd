package com.capgemini.corrected;

public class Example93 {
	private String name;
	private int age;
	private int salary;
	
	public Example93(String name, int age) {
		setName(name);
		setAge(age);
		setSalary(2000);
	}
	public Example93(String name, int age, int salary) {
		setSalary(salary);
		this(name, age);
	}
}
